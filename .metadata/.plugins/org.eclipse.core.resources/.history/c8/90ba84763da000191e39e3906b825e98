
import javax.swing.*;

public class ButtonClicks extends JButton {

    int coordX;
    int coordY;

    public ButtonClicks(String buttonText, int coordX, int coordY) {
        super(buttonText);
        this.coordX = coordX;
        this.coordY = coordY;
    }


    public int getCoordX() {
        return coordX;
    }

    public int getCoordY() {
        return coordY;
    }
}


